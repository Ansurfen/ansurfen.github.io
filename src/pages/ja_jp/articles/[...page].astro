---
import { getArticlePaths } from "@/lib/articles";
import ArticlesLayout from "@/layouts/articles-layout.astro";

export const getStaticPaths = await getArticlePaths("jaJPBlog");

const { page } = Astro.props;
---

<ArticlesLayout
  collection="jaJPBlog"
  title="ブログ共有"
  description="不定期分享前端、後端、全スタック開発等の記事"
  seriesTitle="シリーズ"
  categoriesTitle="カテゴリー"
  articlesTitle="記事"
  basePath="/ja_jp/articles"
  page={page}
/>
